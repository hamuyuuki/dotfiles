--regex-ruby=/^[\t ]*([A-Z][-_A-Za-z0-9]*::)*([A-Z][-_A-Za-z0-9]*)[\t ]*=/\2/d,constant definitions/
--regex-ruby=/^[\t ]*(scope) :([a-zA-Z][-_A-Za-z0-9]*),[\t ]*/\2/s,scope methods/
--regex-ruby=/^[\t ]*(def)[\t ]*(self\.)([a-zA-Z][-_A-Za-z0-9]*)/\3/C,class methods/
--regex-ruby=/^[\t ]*(def)[\t ]*(self\.)([a-zA-Z][-_A-Za-z0-9]*)\!/\3/b,bang methods/
--regex-ruby=/^[\t ]*(def)[\t ]*(self\.)([a-zA-Z][-_A-Za-z0-9]*)\?/\3/q,question methods/
--exclude=.bundle
--exclude=log
--exclude=db
--exclude=public
--extra=+q
